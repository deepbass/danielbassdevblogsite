{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "profiles_danbasscdnprofile_name": {
            "defaultValue": "danbasscdnprofile",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Cdn/profiles",
            "apiVersion": "2016-04-02",
            "name": "[parameters('profiles_danbasscdnprofile_name')]",
            "location": "WestUs",
            "sku": {
                "name": "Standard_Akamai"
            },
            "properties": {
                "provisioningState": "Succeeded",
                "resourceState": "Active"
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/endpoints",
            "apiVersion": "2016-04-02",
            "name": "[concat(parameters('profiles_danbasscdnprofile_name'), '/dbdev')]",
            "location": "WestUs",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_danbasscdnprofile_name'))]"
            ],
            "properties": {
                "originHostHeader": "danielbassdevblogprd.z33.web.core.windows.net",
                "isHttpAllowed": true,
                "isHttpsAllowed": true,
                "queryStringCachingBehavior": "IgnoreQueryString",
                "origins": [
                    {
                        "name": "danielbassdevblogprd-blob-core-windows-net",
                        "properties": {
                            "hostName": "danielbassdevblogprd.z33.web.core.windows.net",
                            "httpPort": 80,
                            "httpsPort": 443
                        }
                    }
                ],
                "contentTypesToCompress": [
                    "text/plain",
                    "text/html",
                    "text/css",
                    "text/javascript",
                    "application/x-javascript",
                    "application/javascript",
                    "application/json",
                    "application/xml"
                ],
                "isCompressionEnabled": true
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/endpoints/customdomains",
            "apiVersion": "2016-04-02",
            "name": "[concat(parameters('profiles_danbasscdnprofile_name'), '/dbdev/www-danielbass-dev')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/endpoints', parameters('profiles_danbasscdnprofile_name'), 'dbdev')]",
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_danbasscdnprofile_name'))]"
            ],
            "properties": {
                "hostName": "www.danielbass.dev"
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/endpoints/origins",
            "apiVersion": "2016-04-02",
            "name": "[concat(parameters('profiles_danbasscdnprofile_name'), '/dbdev/danielbassdevblogprd-blob-core-windows-net')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/endpoints', parameters('profiles_danbasscdnprofile_name'), 'dbdev')]",
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_danbasscdnprofile_name'))]"
            ],
            "properties": {
                "hostName": "danielbassdevblogprd.z33.web.core.windows.net",
                "httpPort": 80,
                "httpsPort": 443
            }
        }
    ]
}